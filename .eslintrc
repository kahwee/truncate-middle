{
  "extends": "standard",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
  },
  "rules": {
    "complexity": [1, 12],
    "max-depth": [1, 3],
    "max-nested-callbacks": [2, 4],
    "max-params": [1, 4],
    "max-statements": [1, 30]
  },
  "globals": {
  }
}
